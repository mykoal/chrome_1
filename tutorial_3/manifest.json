{
    "manifest_version": 3,
    "name": "Focus Mode",
    "description": "Enable focus mode on Chrome's official Extensions and Chrome Web Store documentation.",
    "version": "1.0",
    "icons": {
      "16": "images_3/icon-16.png",
      "32": "images_3/icon-32.png",
      "48": "images_3/icon-48.png",
      "128": "images_3/icon-128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": {
            "16": "images_3/icon-16.png",
            "32": "images_3/icon-32.png",
            "48": "images_3/icon-48.png",
            "128": "images_3/icon-128.png"
        }
    },
    "permissions": ["activeTab", "scripting"],
    "commands": {
        "_execute_action": {
          "suggested_key": {
            "default": "Ctrl+B",
            "mac": "Command+B"
          }
        }
      }

  }